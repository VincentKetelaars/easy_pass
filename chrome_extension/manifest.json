{
  "manifest_version": 2,
  "name": "EasyPass",
  "description": "This extension allows you to create a strong password, using only a simple password",
  "version": "1.0",
  "icons": { "16": "icon16.png",
              "48": "icon48.png",
             "128": "icon128.png" },
  "browser_action": {
    "default_icon": { "16": "icon16.png",
                      "48": "icon48.png",
          			     "128": "icon128.png" },
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}